{
  "name": "flutterwave-vue-v3",
  "version": "1.0.5",
  "private": false,
  "description": "Flutterwave official Vue library to accept payment via Card , USSD, QrCode etc.",
  "author": "Flutterwave",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flutterwave/Flutterwave-Vue-v3.git"
  },
  "homepage": "https://github.com/Flutterwave/Flutterwave-Vue-v3",
  "keywords": [
    "Flutterwave",
    "Vue",
    "Flw",
    "Fintech",
    "Fluterwave"
  ],
  "license": "MIT",
  "main": "dist/flutterwave-vue-v3.ssr.js",
  "browser": "dist/flutterwave-vue-v3.esm.js",
  "module": "dist/flutterwave-vue-v3.esm.js",
  "unpkg": "dist/flutterwave-vue-v3.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "README.md"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "pug-code-gen": ">=2.0.3",
    "rollup": "^2.40.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/Flutterwave/Flutterwave-Vue-v3/issues"
  },
  "directories": {
    "example": "example"
  }
}
